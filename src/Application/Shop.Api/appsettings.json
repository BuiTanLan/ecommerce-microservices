{
  "Logging": {
    "level": "information"
  },
  "UseInMemoryDatabase": false,
  "ConnectionStrings": {
    "ShopConnection": "Server=.\\sqlexpress;Database=ShopDB;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "AppOptions": {
    "Name": "Shop Application",
    "Description": "Shop App based on .Net Core",
    "ApiAddress": "http://localhost:5000"
  },
  "AllowedHosts": "*",
  "easycaching": {
    "mem": {
      "MaxRdSecond": 120,
      "EnableLogging": true,
      "LockMs": 5000,
      "SleepMs": 300,
      "DBConfig": {
        "SizeLimit": 10000,
        "ExpirationScanFrequency": 60,
        "EnableReadDeepClone": true,
        "EnableWriteDeepClone": false
      }
    }
  },
  "PolicyConfig": {
    "RetryCount": 3,
    "BreakDuration": 30,
    "TimeOutDuration": 15
  },
  "SwaggerOptions": {
    "Title": "Shop Apis"
  }
}